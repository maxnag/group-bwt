{
    "plugins": ["@prettier/plugin-php"],
    "braceStyle": "per-cs",
    "endOfLine": "lf",
    "insertPragma": false,
    "parser": "php",
    "phpVersion": "8.2",
    "printWidth": 140,
    "proseWrap": "preserve",
    "requirePragma": false,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingCommaPHP": true,
    "useTabs": false
}
